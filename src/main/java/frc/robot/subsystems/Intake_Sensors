package frc.robot.subsystems;

import edu.wpi.first.wpilibj.DigitalInput; // DigitalInput class
import edu.wpi.first.wpilibj2.command.SubsystemBase;

public class Intake_Sensors extends SubsystemBase {
  private static DigitalInputSubsystem instance;
  public static DigitalInputSubsystem getInstance() {
    if (instance == null) {
      instance = new DigitalInputSubsystem();
    }
    return instance;
  }
  private final DigitalInput limitSwitch; // DigitalInput instance

  public DigitalInputSubsystem() {
    // Initialize DigitalInput on channel 5
    limitSwitch = new DigitalInput(5);
  }

  @Override
  public void periodic() {
    boolean isSwitchPressed = limitSwitch.get(); // You can use the limitSwitch in your periodic code
    System.out.println("Limit Switch Status: " + isSwitchPressed);
  }
}

